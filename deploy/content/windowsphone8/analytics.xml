<?xml version="1.0" encoding="utf-8" ?>
<!--More info at https://developers.google.com/analytics/devguides/collection/android/v2/parameters-->
<resources>
  <!--Replace placeholder ID with your tracking ID-->
  <string name="ga_trackingId">UA-XXXX-Y</string>
  <!--Replace placeholder name with your app name-->
  <string name="ga_appName">Test</string>
  <!--Replace placeholder version with your app version-->
  <string name="ga_appVersion">1.0.0.0</string>
  <!--Enable writing of debug information to debug window log, useful for troubleshooting your implementation-->
  <bool name="ga_debug">false</bool>
  <!--The dispatch period in seconds.-->
  <integer name="ga_dispatchPeriod">30</integer>
  <!--The dispatch period in seconds.-->
  <string name="ga_sampleFrequency">100.0</string>
  <!--Tells Google Analytics to anonymize the information sent by the tracker objects by removing the last octet of the IP address prior to its storage. Note that this will slightly reduce the accuracy of geographic reporting.-->
  <bool name="ga_anonymizeIp">false</bool>
  <!--Enable automatic exception tracking-->
  <bool name="ga_reportUncaughtExceptions">false</bool>
  <!--The amount of time (in seconds) your application can stay in the background before the session is ended. Negative value disables EasyTracker session management.-->
  <integer name="ga_sessionTimeout">30</integer>
  <!--Enable automatic app lifetime tracking (e.g. suspend/resume)-->
  <bool name="ga_autoAppLifetimeTracking">false</bool>
</resources>